{
  "name": "api-key-aggregetor-dev",
  "displayName": "Gemini API Key Aggregator Proxy Plus",
  "description": "Aggregates multiple Google Gemini API keys to bypass rate limits and improve concurrency for VS Code extensions like Cline and Roo Code.",
  "version": "0.0.2",
  "publisher": "rlong",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "AI"
  ],
  "keywords": [
    "Gemini",
    "API Key",
    "Aggregator",
    "Proxy",
    "Cline",
    "Roo Code",
    "Continue",
    "Rate Limit",
    "Concurrency"
  ],
  "activationEvents": [
    "onCommand:geminiAggregator-dev.runserver",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "geminiAggregator-dev.runserver",
        "title": "Run Server"
      },
      {
        "command": "geminiAggregator-dev.addApiKey",
        "title": "Gemini: Add API Key"
      },
      {
        "command": "geminiAggregator-dev.listApiKeys",
        "title": "Gemini: List API Keys"
      },
      {
        "command": "geminiAggregator-dev.modifyApiKey",
        "title": "Gemini: Modify API Key"
      },
      {
        "command": "geminiAggregator-dev.deleteApiKey",
        "title": "Gemini: Delete API Key"
      }
    ],
    "configuration": {
      "title": "Gemini API Key Aggregator Configuration",
      "properties": {
        "geminiAggregator-dev.port": {
          "type": "number",
          "default": 3146,
          "description": "Port to be used by the proxy server."
        }
      }
    
    }
  },

  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamzYang/api-key-aggregetor"
}
}
