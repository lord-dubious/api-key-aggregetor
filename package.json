{
  "name": "api-key-aggregetor-dev",
  "displayName": "Gemini API Key Aggregator Proxy Plus",
  "description": "Aggregates multiple Google Gemini API keys to bypass rate limits and improve concurrency for VS Code extensions like Cline and Roo Code.",
  "version": "0.0.2",
  "publisher": "rlong",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "AI"
  ],
  "keywords": [
    "Gemini",
    "API Key",
    "Aggregator",
    "Proxy",
    "Cline",
    "Roo Code",
    "Continue",
    "Rate Limit",
    "Concurrency"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "extensionKind": ["workspace", "ui"],
  "browser": "./out/extension-web.js",
  "contributes": {
    "commands": [
      {
        "command": "geminiAggregator-dev.runserver",
        "title": "Gemini: Run Server"
      },
      {
        "command": "geminiAggregator-dev.addApiKey",
        "title": "Gemini: Add API Key"
      },
      {
        "command": "geminiAggregator-dev.removeApiKey",
        "title": "Gemini: Remove API Key"
      },
      {
        "command": "geminiAggregator-dev.listApiKeys",
        "title": "Gemini: List API Keys"
      },
      {
        "command": "geminiAggregator-dev.modifyApiKey",
        "title": "Gemini: Modify API Key"
      },
      {
        "command": "geminiAggregator-dev.deleteApiKey",
        "title": "Gemini: Delete API Key"
      },
      {
        "command": "geminiAggregator-dev.viewApiKeyDetails",
        "title": "Gemini: View API Key Details"
      },
      {
        "command": "geminiAggregator-dev.testApiKey",
        "title": "Gemini: Test API Key"
      },
      {
        "command": "geminiAggregator-dev.refreshApiKeys",
        "title": "Gemini: Refresh API Keys"
      },
      {
        "command": "geminiAggregator-dev.openPanel",
        "title": "Gemini: Open Aggregator Panel"
      },
      {
        "command": "geminiAggregator-dev.toggleServer",
        "title": "Gemini: Toggle Server"
      },
      {
        "command": "geminiAggregator-dev.startServer",
        "title": "Gemini: Start Server"
      },
      {
        "command": "geminiAggregator-dev.stopServer",
        "title": "Gemini: Stop Server"
      },
      {
        "command": "geminiAggregator-dev.showLogs",
        "title": "Gemini: Show Logs"
      },
      {
        "command": "geminiAggregator-dev.refreshAll",
        "title": "Gemini: Refresh All"
      },
      {
        "command": "geminiAggregator-dev.addProxy",
        "title": "Gemini: Add Proxy"
      },
      {
        "command": "geminiAggregator-dev.removeProxy",
        "title": "Gemini: Remove Proxy"
      },
      {
        "command": "geminiAggregator-dev.testProxy",
        "title": "Gemini: Test Proxy"
      },
      {
        "command": "geminiAggregator-dev.assignProxy",
        "title": "Gemini: Assign Proxy"
      },
      {
        "command": "geminiAggregator-dev.refreshProxies",
        "title": "Gemini: Refresh Proxies"
      },
      {
        "command": "geminiAggregator-dev.viewProxyDetails",
        "title": "Gemini: View Proxy Details"
      },
      {
        "command": "geminiAggregator-dev.restartServer",
        "title": "Gemini: Restart Server"
      },
      {
        "command": "geminiAggregator-dev.exportConfig",
        "title": "Gemini: Export Configuration"
      },
      {
        "command": "geminiAggregator-dev.importConfig",
        "title": "Gemini: Import Configuration"
      },
      {
        "command": "geminiAggregator-dev.viewServerStatus",
        "title": "Gemini: View Server Status"
      },
      {
        "command": "geminiAggregator-dev.setProxyAssignmentMode",
        "title": "Gemini: Set Proxy Assignment Mode"
      },
      {
        "command": "geminiAggregator-dev.testRotatingProxy",
        "title": "Gemini: Test Rotating Proxy"
      },
      {
        "command": "geminiAggregator-dev.configureRotatingProxy",
        "title": "Gemini: Configure Rotating Proxy"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "geminiAggregator",
          "title": "Gemini Aggregator",
          "icon": "$(key)"
        }
      ]
    },
    "views": {
      "geminiAggregator": [
        {
          "id": "geminiAggregatorView",
          "name": "Gemini Aggregator",
          "icon": "$(key)",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "Gemini API Key Aggregator Configuration",
      "properties": {
        "geminiAggregator-dev.port": {
          "type": "number",
          "default": 3146,
          "description": "Port to be used by the proxy server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test --no-sandbox",
    "test:setup": "node scripts/setup-test-env.js",
    "test:proxy": "npm run compile && node out/test/proxy-test-runner.js",
    "test:proxy-real": "npm run compile && node out/test/proxy-test-runner.js",
    "test:proxy-servers": "npm run compile && node out/test/proxy-server.js",
    "test:integration": "npm run compile && npx mocha out/test/integration/proxy-per-api-key.test.js --timeout 60000",
    "test:integration-real": "npm run compile && npx mocha out/test/integration/proxy-per-api-key.test.js --timeout 60000"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "socks-proxy-agent": "^8.0.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "google-auth-library": "^10.1.0",
    "https-proxy-agent": "^7.0.6",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^2.7.0",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1",
    "proxy": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamzYang/api-key-aggregetor"
  }
}
